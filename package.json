{
  "name": "nuber-eats-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@hookform/resolvers": "^2.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "apollo": "^2.32.5",
    "autoprefixer": "^10.2.5",
    "google-map-react": "^2.1.10",
    "graphql": "^15.5.0",
    "postcss": "^8.2.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-hook-form": "^7.1.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "subscriptions-transport-ws": "^0.9.19",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
  },
  "scripts": {
    "tailwind:build": "tailwind build ./src/styles/tailwind.css -o ./src/styles/styles.css",
    "start": "yarn apollo:codegen & yarn tailwind:build & react-scripts start",
    "apollo:codegen": "rm -rf src/__generated__ && apollo client:codegen src/__generated__ --target=typescript --outputFlat",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "yarn test -- --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/pages/**/*.tsx",
      "./src/routers/**/*.tsx",
      "./src/components/**/*.tsx"
    ]
  },
  "devDependencies": {
    "@types/google-map-react": "^2.1.1",
    "@types/googlemaps": "^3.43.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "stylelint-config-recommended": "^4.0.0"
  }
}
